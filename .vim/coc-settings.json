  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "dart",
    "go",
    "html",
    "javascript",
    "javascriptreact",
    "js",
    "json",
    "jsx",
    "php",
    "python",
    "sass",
    "scss",
    "ts",
    "tsx",
    "typescript",
    "typescriptreact",
    "vim",
    "yaml"
