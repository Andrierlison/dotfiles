{
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "dart",
    "go",
    "html",
    "javascript",
    "javascriptreact",
    "json",
    "php",
    "python",
    "scss",
    "typescript",
    "typescriptreact",
    "vim",
    "yaml"
  ],
  "emmet.excludeLanguages": [
    "markdown",
    "json",
    "vim",
    "js",
    "jsx",
    "ts",
    "tsx",
    "css"
  ],
  "coc.preferences.colorSupport": true,
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "coc.preferences.extensionUpdateCheck": "daily",
  "coc.preferences.messageLevel": "warning",
  "coc.source.around.shortcut": "Around",
  "coc.source.buffer.shortcut": "Buffer",
  "coc.source.file.shortcut": "File",
  "explorer.autoExpandOptions": ["recursive"],
  "explorer.buffer.showHiddenBuffers": true,
  "explorer.debug": true,
  "explorer.file.showHiddenFiles": true,
  "explorer.filename.colored.enable": false,
  "explorer.icon.enableNerdfont": true,
  "explorer.quitOnOpen": true,
  "explorer.width": 30,
  "flutter.lsp.initialization.closingLabels": true,
  "git.addedSign.hlGroup": "Title",
  "git.addedSign.text": "++",
  "git.changeRemovedSign.hlGroup": "WarningMsg",
  "git.changeRemovedSign.text": "≃≃",
  "git.changedSign.hlGroup": "MoreMsg",
  "git.changedSign.text": "~~",
  "git.removedSign.hlGroup": "WarningMsg",
  "git.removedSign.text": "--",
  "git.showCommitInFloating": true,
  "git.topRemovedSign.text": "‾‾",
  "intelephense.environment.phpVersion": "8.0.0",
  "snippets.shortcut": "Snippets",
  "yank.enableCompletion": false,
  "yank.highlight.duration": 1000,
  "yank.list.maxsize": 100
}
